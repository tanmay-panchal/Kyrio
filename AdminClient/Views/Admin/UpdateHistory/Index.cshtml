@Styles.Render("~/Style/datatable")
<link href="@Url.Content("~/Extension/css/select2.min.css")" rel="stylesheet">
<link href="@Url.Content("~/Extension/css/icheck.css")" rel="stylesheet">
<link href="@Url.Content("~/Extension/css/PNotifyBrightTheme.css")" rel="stylesheet" />
<link href="@Url.Content("~/Extension/css/intlTelInput.css")" rel="stylesheet" />
<link href="@Url.Content("~/Extension/css/daterangepicker.min.css")" rel="stylesheet" />
<div class="animated fadeIn">
    <div class="col-md-12">
        <div class="form-control col-12 p-0 d-flex justify-content-end pb-1 b-0">
            <button type="button" class="btn btn-danger active" id="deleteMuliButton">DELETE</button>
        </div>
        <div class="form-control col-12 p-0 containtTable pt-2">
            <table class="table table-responsive-sm table-striped table-sm datatable no-footer" id="table" style="width:100%">
                <thead>
                    <tr>
                        <th></th>
                        <th>Date</th>
                        <th>Subject</th>
                        <th>Enable</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
    <div class="row">
        <div id="containtButtonCreate">
        </div>
    </div>
</div>
<div class="modal fade" id="actionModal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title" id="TitleModal">Modal title</h3>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="modal-body" style="max-height:70vh; overflow-y:auto">
                <form novalidate="novalidate" id="Form">
                    <div class="form-group">
                        <label class="col-form-label">Date Update</label>
                        <div class="input-group mb-4">
                            <div class="input-group-prepend">
                                <span class="input-group-text"><i class="icon-calendar"></i></span>
                            </div>
                            <input type="text" class="form-control" id="DateUpdate" isdate name="DateUpdate" ispropertiesmodel formatdate="DD/MM/YYYY" />
                        </div>
                    </div>
                    <div class="form-group">
                        <input type="hidden" id="UpdateID" name="UpdateID" ispropertiesmodel ispropertiesidmodel />
                        <label class="col-form-label">SUBJECT</label>
                        <input type="text" class="form-control" id="Subject" name="Subject" placeholder="Subject" ispropertiesmodel maxlength="200">
                    </div>

                    <div class="form-group">
                        <label class="col-form-label">DESCRIPTION</label>
                        <textarea class="form-control" rows="2" id="Detail" name="Detail" placeholder="Description" ispropertiesmodel maxlength="400"></textarea>
                    </div>
                    <div class="form-group">
                        <label class="col-form-label">LINK</label>
                        <input type="text" class="form-control" id="Link" name="Link" placeholder="" ispropertiesmodel maxlength="200">
                    </div>
                    <div class="form-group">
                        <label class="switch switch-icon switch-pill switch-primary">
                            <input type="checkbox" class="switch-input" id="IsShow" name="IsShow" ispropertiesmodel>
                            <span class="switch-label" data-on="" data-off=""></span>
                            <span class="switch-handle"></span>
                        </label>
                        <label class="col-form-label">ENABLE</label>
                    </div>
                </form>
            </div>
            <div class="modal-footer">

                <div class="col-fhd-3 col-xlg-3 col-md-3 col-sm-6 col-xs-6">

                </div>
                <div class="col-fhd-3 col-xlg-3 col-md-3 col-sm-6 col-xs-6 hidden-xs"></div>
                <div class="col-fhd-3 col-xlg-3 col-md-3 col-sm-6 col-xs-6 hidden-xs">
                    <button type="button" class="btn btn-block btn-outline-dark" data-dismiss="modal">CANCEL</button>
                </div>
                <div class="col-fhd-3 col-xlg-3 col-md-3 col-sm-6 col-xs-6">
                    <button type="button" class="btn btn-block btn-success" id="actionButton" data-style="slide-right">SAVE</button>
                </div>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<script src="@Url.Content("~/Extension/js/icheck.min.js")"></script>
@Scripts.Render("~/Scripts/PNotify")
@Scripts.Render("~/Scripts/datatable")
<script src="@Url.Content("~/Extension/js/select2.min.js")"></script>
<script src="@Url.Content("~/Scripts/admin/updatehistory.js")"></script>
<script src="@Url.Content("~/Extension/js/intlTelInput.min.js")"></script>
<script src="@Url.Content("~/Extension/js/daterangepicker.min.js")"></script>