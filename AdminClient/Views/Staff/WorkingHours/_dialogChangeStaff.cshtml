<div class="modal fade" id="excuteChangeStaff" ismodal>
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <div class="modal-title">
                    <h3 id="TitleModalChangeStaff" class="text-center"></h3>
                    <span class="text-center">Assign which staff can be booked at this location</span>
                </div>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="modal-body">
                <form novalidate="novalidate" id="excuteFormChangeStaff">
                    <div class="form-group">
                        <div class="col-fhd-12 col-xlg-12 col-md-12 col-sm-12 col-xs-12">
                            <input type="checkbox" id="checkAllChangeStaff" /> <span style="margin-left: 5px">Select All</span>
                        </div>
                    </div>
                    <div class="form-group" id="containtChangeStaff"></div>
                </form>
            </div>
            <div class="modal-footer text-right">
                <div class="col-fhd-6 col-xlg-6 col-md-6 col-sm-12 col-xs-12">
                </div>
                <div class="col-fhd-3 col-xlg-3 col-md-3 col-sm-12 col-xs-12">
                    <div class="form-group">
                        <button type="button" class="btn btn-block btn-outline-dark" data-dismiss="modal">CANCEL</button>
                    </div>
                </div>
                <div class="col-fhd-3 col-xlg-3 col-md-3 col-sm-12 col-xs-12">
                    <div class="form-group">
                        <button type="button" class="btn btn-block btn-custome" id="excuteButtonChangeStaff" data-style="slide-right" isbuttonsave>SAVE</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!--#region Include JS-->
<script src="@Url.Content("~/Scripts/staff/workinghours/excutedialogchangestaff.js")"></script>
<!--#endregion-->